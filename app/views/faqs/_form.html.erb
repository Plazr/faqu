<%= form_for(@faq) do |f| %>
	<p>
		<%= f.label :question, 'Question' %><br />
		<%= f.text_field :question %>
	</p>
	<p>
		<%= f.label :answer, 'Answer' %><br />
		<%= f.text_area :answer %>
	</p>
	<p>
		<%= f.label :tag_list, 'Tags' %><br />
		<%= f.text_field :tag_list %>
	</p>
	<p>
		<%= f.submit 'Submit' %>
	</p>
<% end %>
